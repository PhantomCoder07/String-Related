#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    printf ("Enter the string: ");
    gets(str);
    int l=strlen(str);
    for (int i=0; i<l; i++)
    {
        for (int j=0; j<l; j++)
        {
            if (str[i]<str[j])
            {
                char temp=str[i];
                str[i]=str[j];
                str[j]=temp;
            }
        }
    }
    printf("Sorted string: %s\n",str);
    return 0;
}
