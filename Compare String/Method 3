#include <stdio.h>
int main()
{
    char str1[100],str2[100];
    int ans=0;
    printf("Enter 1st string: ");
    scanf("%s",str1);
    printf("Enter 2nd string: ");
    scanf("%s",str2);
    for (int i=0,j=0; (str1[i]!='\0' || str2[j]!='\0'); i++,j++)
    {
        if (str1[i]!=str2[j])
        {
            ans=1;
            break;
        }
        else
            continue;
    }
    if (ans==0)
        printf("Strings are equal\n");
    else
        printf("Strings are different\n");
    return 0;
}
