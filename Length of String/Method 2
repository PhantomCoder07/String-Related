#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    int i,count=0;
    printf ("Enter the string: ");
    gets (str);
    for (i=0; str[i]!='\0'; i++)
    {
        if (str[i]!='\0')
            count++;
    }
    printf ("Total character in string is: %d\n",count);
    return 0;
}
