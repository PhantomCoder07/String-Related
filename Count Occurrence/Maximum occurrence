#include <stdio.h>
#include <string.h>
int main()
{
    char str[100],maxchar;
    int i,j,l,max=0,count;
    printf ("Enter the string: ");
    gets(str);
    l=strlen(str);
    for (i=0; i<l; i++)
    {
        count=0;
        for (j=0; j<l; j++)
        {
            if (str[i]==str[j] && str[i]!=' ')
            {
                count++;
            }
        }
        if (max<count)
        {
            max=count;
            maxchar=str[i];
        }
    }
    printf ("The max occurred character: '%c'\nRepeat: %d times\n",maxchar,max);
    return 0;
}
