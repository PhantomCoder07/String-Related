#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    printf ("Enter the string: ");
    gets (str);
    int l=strlen(str);
    for (int i=0; i<l; i++)
    {
        if ((str[i]>='a' && str[i]<='z') || (str[i]>='A' && str[i]<='Z'))
            printf("%c",str[i]);
        else 
            printf("\n");
    }
    return 0;
}
