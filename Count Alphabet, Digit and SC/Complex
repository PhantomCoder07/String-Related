#include <stdio.h>
#include <string.h>
int main()
{
    char str[100],ch;
    int i,l,c=0,d=0,s=0,v=0;
    printf ("Enter the string: ");
    gets(str);
    l= strlen(str);
    for(i=0; i<l; i++)
    {
        ch=str[i];
        if((ch>='a' && ch<='z') || (ch>='A' && ch<='Z'))
        {
            if (ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
                v++;
            else
                c++;
        }
        else if (ch>='0' && ch<='9')
            d++;
        else
            s++;
    }
    printf ("\nTotal alphabet         : %d\n",c+v);
    printf ("Total vowel alphabet     : %d\n",v);
    printf ("Total consonant alphabet : %d\n",c);
    printf ("Total digit              : %d\n",d);
    printf ("Total special character  : %d\n",s);
    return 0;
}
