#include <stdio.h>
int main()
{
    char str1[100],str2[100];
    printf("Enter 1st string: ");
    gets(str1);
    printf("Enter 2nd string: ");
    gets(str2);
    int i=0,j=0;
    while (str1[i]!='\0')
    {
        i++;
    }
    while (str2[j]!='\0')
    {
        str1[i++]=str2[j++];
    }
    str1[i]='\0';
    printf("Concatenate string is: %s\n",str1);
    return 0;
}
