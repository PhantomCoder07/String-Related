#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    int i,ans=0;
    printf("Enter the string: ");
    gets(str);
    int n=strlen(str);
    for (i=0; i<n/2; i++)
    {
        if (str[i]!=str[n-1-i])
        {
            ans=1;
            break;
        }
    }
    if (ans==0)
        printf("The string is Palindrome\n");
    else
        printf("The string is not Palindrome\n");
    return 0;
}
