#include <stdio.h>
#include <string.h>
int main()
{
    char str1[100],str2[100];
    printf("Enter the string: ");
    gets(str1);
    strcpy(str2,str1);
    strrev(str2);
    int ans=strcmp(str1,str2);
    if (ans==0)
        printf("The string is Palindrome\n");
    else
        printf("The string is not Palindrome\n");
    return 0;
}
