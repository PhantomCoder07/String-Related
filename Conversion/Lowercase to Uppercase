#include <stdio.h>
#include <ctype.h>
int main()
{
    char str[100];
    printf("Enter the string: ");
    gets(str);
    for (int i=0; str[i]!='\0'; i++)
    {
        if (isalpha(str[i]))
            str[i]=toupper(str[i]);
        else
            continue;
    }
    printf("After conversion: %s\n",str);
    return 0;
}
