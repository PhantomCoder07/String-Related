#include <stdio.h>
#include <ctype.h>
int main()
{
    char str[100];
    printf("Enter the string: ");
    gets(str);
    for (int i=0; str[i]!='\0'; i++)
    {
        if (str[i]>='A' && str[i]<='Z')
            str[i]=tolower(str[i]);
        else if (str[i]>='a' && str[i]<='z')
            str[i]=toupper(str[i]);
        else
            continue;
    }
    printf("After conversion: %s\n",str);
    return 0;
}
