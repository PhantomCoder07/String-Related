#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    printf ("Enter the string: ");
    gets (str);
    int l=strlen(str);
    for (int i=0; i<l; i++)
    {
        if (i==0 && str!=' ')
            printf("%c ",str[i]);
        if (str[i]==' ')
            printf("%c ",str[i+1]);
    }
    return 0;
}
