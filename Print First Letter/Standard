#include <stdio.h>
int main()
{
    char str[100];
    printf ("Enter the string: ");
    gets (str);
    int p=1;
    for (int i=0; str[i]!='\0'; i++)
    {
        if (str[i]!=' ' && p==1)
        {
            printf("%c ",str[i]);
            p=0;
        }
        if (str[i]==' ')
            p=1;
    }
    return 0;
}
